	"Print log info to console": {
		"prefix": "logi",
		"body": [
			"logging.info(${1:var})",
			"$2"
		],
		"description": "Log output to console"
	},
	"Print log to console": {
		"prefix": "logiv",
		"body": [
			"logging.info(f'${1:var} = ${1:var}')",
			"$2"
		],
		"description": "Log output to console"
	},
	"Print log to console": {
		"prefix": "logdv",
